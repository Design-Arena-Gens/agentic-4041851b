{
    "name": "agentic/autovalidation-socketio",
    "description": "Hyperf Socket.IO server with automatic validation behavior",
    "type": "project",
    "require": {
        "php": "^8.1",
        "hyperf/config": "^3.1",
        "hyperf/context": "^3.1",
        "hyperf/contract": "^3.1",
        "hyperf/di": "^3.1",
        "hyperf/logger": "^3.1",
        "hyperf/event": "^3.1",
        "hyperf/server": "^3.1",
        "hyperf/socketio-server": "^3.1",
        "hyperf/support": "^3.1",
        "hyperf/validation": "^3.1",
        "psr/container": "^2.0",
        "psr/log": "^3.0",
        "vlucas/phpdotenv": "^5.6"
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "scripts": {
        "start": "php bin/hyperf.php start",
        "server:dev": "php bin/hyperf.php start"
    }
}
